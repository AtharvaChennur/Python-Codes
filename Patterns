for i in range(0,5):
    for j in range(0,i):
        print("*",end="")
    print("")
    
    
Pattern 1   
*
**
***
****


for i in range(0,4):
    for j in range(5-i):
        print("*" , end = "")
    print("")
   Pattern 2
